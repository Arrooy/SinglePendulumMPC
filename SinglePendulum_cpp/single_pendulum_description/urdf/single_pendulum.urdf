<?xml version="1.0" encoding="utf-8"?>
<!-- Versio adaptada del DoublePendulum feta per l'Adria Arroyo-->
<robot
  name="1dof_planar">
  <link
    name="base_link">
    

    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="/opt/openrobots/share/example-robot-data/robots/single_pendulum_description/meshes/base_link.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.96078 1 0 1" />
      </material>
    </visual>

    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="/opt/openrobots/share/example-robot-data/robots/single_pendulum_description/meshes/base_link.stl" />
      </geometry>
    </collision>
  </link>

  <link
    name="link1">
    <inertial>
      <origin
        xyz="0.0 0.0 0.1235"
        rpy="0 0 0" />

      <mass value="0.14" />
      <inertia
        ixx="2.135315E-3"
        ixy="0"
        ixz="0"
        iyy="2.135315E-3"
        iyz="0"
        izz="0" />
    </inertial>
  
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="/opt/openrobots/share/example-robot-data/robots/single_pendulum_description/meshes/link1.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="1 0.61961 0.58824 1" />
      </material>
    </visual>
    
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="/opt/openrobots/share/example-robot-data/robots/single_pendulum_description/meshes/link1.stl" />
      </geometry>
    </collision>
  </link>

  <joint
    name="joint1"
    type="revolute">
    <origin
      xyz="0 0 0"
      rpy="0 0 0" />
    
    <parent link="base_link" />
    <child link="link1" />
    
    <axis
      xyz="1 0 0" />

    <limit
      lower="0"
      upper="0"
      effort="0"
      velocity="0" />

    <dynamics damping="0.05" />
  </joint>
</robot>
